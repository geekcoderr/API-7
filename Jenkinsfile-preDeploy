pipeline {
    agent any
    environment{
        GITD=env.
    }
    parameters {
        string(name: 'PERSON', defaultValue: '', description: '')
    }
    stages {
        stage('Example') {
            steps {
               sh("echo ${returnStdout: true, script: 'git log -1 --oneline'}")
            }
        }
    }
}